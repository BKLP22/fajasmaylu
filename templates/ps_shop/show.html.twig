{% extends 'base.html.twig' %}

{% block title %}PsShop{% endblock %}

{% block body %}
    <h1>PsShop</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdShop</th>
                <td>{{ ps_shop.idShop }}</td>
            </tr>
            <tr>
                <th>IdShopGroup</th>
                <td>{{ ps_shop.idShopGroup }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ps_shop.name }}</td>
            </tr>
            <tr>
                <th>IdCategory</th>
                <td>{{ ps_shop.idCategory }}</td>
            </tr>
            <tr>
                <th>ThemeName</th>
                <td>{{ ps_shop.themeName }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ ps_shop.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Deleted</th>
                <td>{{ ps_shop.deleted ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_ps_shop_index') }}">back to list</a>

    <a href="{{ path('app_ps_shop_edit', {'idShop': ps_shop.idShop}) }}">edit</a>

    {{ include('ps_shop/_delete_form.html.twig') }}
{% endblock %}
